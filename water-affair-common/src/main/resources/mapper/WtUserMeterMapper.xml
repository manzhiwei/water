<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.welltech.waterAffair.repository.WtUserMeterMapper">
    <resultMap id="BaseResultMap" type="com.welltech.waterAffair.domain.entity.WtUserMeter">
        <id column="userNameId" jdbcType="INTEGER" property="userNameId" />
        <result column="meterId" jdbcType="INTEGER" property="meterId" />
        <result column="inLine" jdbcType="INTEGER" property="inLine" />
        <result column="inPower" jdbcType="INTEGER" property="inPower" />
        <result column="inLineType" jdbcType="INTEGER" property="inLineType" />
        <result column="inPowerType" jdbcType="INTEGER" property="inPowerType" />
    </resultMap>
    <insert id="save" parameterType="com.welltech.waterAffair.domain.entity.WtUserMeter">
        insert into userMeter (userNameId,meterId,inLineType,inLine,inPowerType)
        values(#{userNameId},#{meterId},#{inLineType},#{inLine},#{inPowerType})
    </insert>

</mapper>